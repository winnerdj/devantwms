
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Create Dispatch</h3>
            </div>
             <div id="search_div">
               <form method="get" action="/dispatch/create">
              <h4 class="filter-title">Filter</h4>
              <div class="row">
                  <div class="col-12 col-md-2">
                    <div class="form-group">
                      <label class="control-sidebar-subheading">
                        ODO No.
                      </label>
                      <input type="text" autocomplete="off" class="form-control" name="ASNNo" value="{{ASNNo_inp}}">
                    </div>
                  </div>
                  <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Shipment No
                    </label>
                    <input type="text" autocomplete="off" class="form-control" name="ASNPrimaryRefDocNo" value="{{PrimaryRefDocNo_inp}}">
                  </div>
                </div>
                

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date From
                    </label>
                    <input type="date" class="form-control" name="paramDate1" value="{{paramdate1_inp}}">
                  </div>
                </div>

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date to
                    </label>
                    <input type="date" class="form-control" name="paramDate2" value="{{paramDate2_inp}}">
                  </div>
                </div>
                
              
              </div><!--end of row-->
              <div class="row">
                <div class="col-12 col-md-3">
                  <div class="form-group">
                    <button type="submit" class="btn btn-success btn-sm"  >
                      <span class="glyphicon glyphicon-search" aria-hidden="true"> </span> Search
                    </button>
                  </div>
                </div>
                
              </div><!--end of row-->  
              </form>
            </div>
            
            
            <!-- /.box-header -->
            <div class="box-body">
              <table  class="table table-bordered table-hover table-responsive table-pagination" style="margin-top: 20px;">
                <thead>
                <tr>
                  <th data-sortable="true">PickPlan No</th>
                  <th data-sortable="true">Shipment No</th>
                  <th data-sortable="true">ODO No</th>
                  <th data-sortable="true">Primary Ref Doc</th>
                  <th data-sortable="true">Created Date</th>
                  <th data-sortable="true">Created By</th>
                  <th data-sortable="true">Status</th>
                </tr>
                </thead>
                <tbody>
               {{#each pickplan}}
                  <tr>
                    <td><a href="#" class="create_btn">{{PickPlanNo}}</a></td>
                    <td>{{ShipmentNo}}</td>
                    <td>{{ODONo}}</td>
                    <td data-id="{{UUID}}">{{ODOPrimaryRefDocNo}}</td>
                    <td>{{dateFormat CreatedDateTime  "YYYY-MM-DD  h:mm A"}}</td>   
                    <td>{{ CreatedBy }}</td>  
                    <td>{{Status}}</td>                       
                  </tr>
                {{else}}
                {{/each}}
                </tbody>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
<form id="create_dispatch_form" method="POST" action="/dispatch/create">
    <input type="hidden" name="UUID" id="UUID_inp">
</form>

