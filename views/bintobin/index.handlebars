
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Bin To Bin</h3>
            </div>
             <div id="search_div">
               <form method="get" action="/bin-to-bin">
              <h4 class="filter-title">Filter</h4>
              <div class="row">
                  <div class="col-12 col-md-2">
                    <div class="form-group">
                      <label class="control-sidebar-subheading">
                        Bin To Bin No
                      </label>
                      <input type="text" autocomplete="off" class="form-control" name="BinToBinNo" >
                    </div>
                  </div>
                  

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date From
                    </label>
                    <input type="date" class="form-control" name="paramDate1" value="{{paramdate1_inp}}">
                  </div>
                </div>

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date to
                    </label>
                    <input type="date" class="form-control" name="paramDate2" value="{{paramDate2_inp}}">
                  </div>
                </div>
                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Status
                    </label>
                    <select name="Status" class="form-control">
                        <option></option>
                        <option>Planned</option>
                        <option>Completed</option>
                    </select>
                  </div>
                </div>
                
                
              </div><!--end of row-->

              <div class="row">
                <div class="col-12 col-md-3">
                  <div class="form-group">
                    <button type="submit" class="btn btn-success btn-sm"  >
                      <span class="glyphicon glyphicon-search" aria-hidden="true"> Search</span>
                    </button>
                  </div>
                </div>
              </form>
              </div><!--end of row-->
                
              
            </div>
            
            <!-- /.box-header -->
            <div class="box-body">
              <a type="button" href="/bin-to-bin/create" class="btn btn-success btn-sm" >Create Bin to Bin Plan</a>
              <table  class="table table-bordered table-hover table-responsive table-pagination" style="margin-top: 20px;">
                
                <thead>
                <tr>
                  <th data-sortable="true">Bin to Bin No.</th>
                  <th data-sortable="true">Start Datetime</th>
                  <th data-sortable="true">End Datetime</th>
                  <th data-sortable="true">Created</th>
                  <th data-sortable="true">Status</th>
                </tr>
                </thead>
                <tbody>
               {{#each bintobin_hdr}}
                  <tr>
                    <td><a href="/bin-to-bin/bintobin_dtl/{{BinToBinNo}}">{{BinToBinNo}}</a></td>
                    <td>{{#if StartDatetimeExecution}}{{dateFormat StartDatetimeExecution "YYYY-MM-DD  h:mm A"}}{{/if}}</td>
                    <td>{{#if EndDatetimeExecution}}{{dateFormat EndDatetimeExecution "YYYY-MM-DD  h:mm A"}}{{/if}}</td>
                    <td>{{dateFormat CreatedDateTime  "YYYY-MM-DD  h:mm A"}}</td>
                    <td>{{Status}}</td>                       
                  </tr>
                {{else}}
                {{/each}}
                </tbody>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->


