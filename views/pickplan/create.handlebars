
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Create Pick Plan</h3>
            </div>
             <div id="search_div">
               <form method="get" action="/pickplan/create">
              <h4 class="filter-title">Filter</h4>
              <div class="row">
                  <div class="col-12 col-md-2">
                    <div class="form-group">
                      <label class="control-sidebar-subheading">
                        ODO No.
                      </label>
                      <input type="text" autocomplete="off" class="form-control" name="ODONo" value="{{ODONo_inp}}">
                    </div>
                  </div>
                  <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Primary Ref Doc.
                    </label>
                    <input type="text" autocomplete="off" class="form-control" name="ODOPrimaryRefDocNo" value="{{ODOPrimaryRefDocNo_inp}}">
                  </div>
                </div>
                

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date From
                    </label>
                    <input type="date" class="form-control" name="paramDate1" value="{{paramdate1_inp}}">
                  </div>
                </div>

                <div class="col-12 col-md-2">
                  <div class="form-group">
                    <label class="control-sidebar-subheading">
                      Date to
                    </label>
                    <input type="date" class="form-control" name="paramDate2" value="{{paramDate2_inp}}">
                  </div>
                </div>
               
              
              </div><!--end of row-->
               <div class="row">
                <div class="col-12 col-md-3">
                  <div class="form-group">
                    <button type="submit" class="btn btn-success btn-sm"  >
                      <span class="glyphicon glyphicon-search" aria-hidden="true"> </span> Search
                    </button>
                  </div>
                </div>
                
              </div><!--end of row-->
              </form>  
              
            </div>
            
            
            <!-- /.box-header -->
            <div class="box-body">
              <table id="" class="table table-bordered table-hover table-responsive table-pagination " style="margin-top: 20px;">
                <thead>
                <tr>
                  <th data-sortable="true">ODONo</th>
                  <th data-sortable="true">Shipment No</th>
                  <th data-sortable="true">Transaction Type</th>
                  <th data-sortable="true">Customer</th>
                  <th data-sortable="true">Status</th>
                  <th data-sortable="true">Date Created</th>
                </tr>
                </thead>
                <tbody>
               {{#each outbound_hdr}}
                  <tr>
                    <td><a href="#" class="create_btn">{{ODONo}}</a></td>
                    <td>{{ShipmentNo}}</td>
                    <td>{{TransactionType}}</td>
                    <td data-id="{{UUID}}">{{CustomerCode}}</td>
                    <td >{{Status}}</td>
                    <td>{{dateFormat CreatedDateTime  "YYYY-MM-DD  h:mm A"}}</td>                    
                  </tr>
                {{else}}
                {{/each}}
                </tbody>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

<form action="/pickplan/create" method="POST" id="create_pickplan_form">
    <input type="hidden" name="UUID" id="UUID_inp">
</form>
<!--/upload modal-->


